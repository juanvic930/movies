import re,requests,urlresolver,binascii
def GetLSProData(page_data,Cookie_Jar,m,url='$doregex[link]'):
  if 'rapid' in url:
    source=requests.get(url,headers={'User-Agent':'Mozilla/5.0'}).content.decode('ascii','ignore')
    if '1080p' in source:
      return re.findall('id="videojs".*?source.src=[\'"](.*?)[\'"]',requests.get(url+'&q=1080p',headers={'User-Agent':'Mozilla/5.0'}).content.decode('ascii','ignore'))[0]
    elif '720p' in source:
      return re.findall('id="videojs".*?source.src=[\'"](.*?)[\'"]',requests.get(url+'&q=720p',headers={'User-Agent':'Mozilla/5.0'}).content.decode('ascii','ignore'))[0]
  elif 'openload' in url:
    return urlresolver.resolve(url)
  elif 'ok.ru' in url:
    return urlresolver.resolve(url)
  else:
    hex=url.encode('hex');dcode=binascii.a2b_hex(hex);return re.findall('(.*)',dcode)[0]
