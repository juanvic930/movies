import re ,requests
import urlresolver
def parseint(string):
    return int(''.join([x for x in string if x.isdigit()]))

def GetLSProData(page_data,Cookie_Jar,m):

  values = re.search(r'service=="OnlineTheVideos"\)\{h=dec\("(.*?)"\)\+dec\("(.*?)"', page_data, re.DOTALL)
  var1=values.group(1)
  var2=values.group(2)
  var1+=''.join(' ')
  var1+=''.join(var2)
  str=var1
  d = "" ; str = str.split(" ")
  i=0
  for i in range(0,len(str)): 
   pp=(parseint(str[i])-7)
   d+=''.join(unichr(pp))
  
  streamLink = urlresolver.resolve(d)
  return streamLink
]]></expres>
