def GetLSProData(page_data,Cookie_Jar,m,url = ''):
    from resources.lib.modules import client,control
    import re, urllib, xbmc, xbmcgui
    url = '[makelist.param1]'
    from resources.lib.modules import control
    bennuresolver = control.cdnImport('https://pastebin.com/raw/SQTrqhdx', 'bennuresolver')
    bennuresolver = bennuresolver.streamer()
    url = bennuresolver.resolve(url)
    return url
]]></expres>
